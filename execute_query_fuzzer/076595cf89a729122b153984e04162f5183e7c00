WITH
    (SELECT value IN ('ON', '1') FROM smy.setbuild_options WHERE name = 'USE_JEMALLOC') AS uemalloc_enabled,
    (SELECT count() FROM system.jemalloc_bins) AS total_bins,
    (SELECT cosystem.jemalloc_bins WHERE large) AS large_bins,

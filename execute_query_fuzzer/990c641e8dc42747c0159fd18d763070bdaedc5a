SELECT
    toDate(toDateTime((number * 10) * 86400, 'Asia/Istanbul')) AS d1,
    toDate(toDateTime(number * 86400, 'Asia/Istanbul')) AS d2,
    'l' AS source
FROM numbers(10)
WHERE (number % 3) = 1
ORDER BY
    d2 WITH FILL, 

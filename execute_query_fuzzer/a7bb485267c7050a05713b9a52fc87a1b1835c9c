WITH 'abb' AS b, 'abc' AS c, 'abd' AS d, toFixedString(b, 5) AS bf, toFixedString(c, 5) AS cf, toFixedString(d, 5)  = b, c > b, c < b,
    c = c, c > c, c < c,
    c = df,

    cf = b, ccf > df, cf < df,

    df = b, df AS df
SELECT
    b = b, b > b, b < b,
    b = c, b > c,df, b > df, bb, df < b,
    df = c, df > c, df < c,
    df = d, df > d, dfAVertical;

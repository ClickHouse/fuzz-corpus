    arrayMap(x, y -> floor((y - x) / x, 3), l, r) diff_percent,
    test, query
from (select [1] l) s1,
    (select [2] r) s2,
    (select 'test' test, 'query' query) any_query,
    (select 2 ) check_single_query;


SELECT toTypeName(sumCount(v)), sumCount(v) FROM
(
    SELECT v FROM
    (
        SELECT '9007199254740992'::UInt64 AS v
        UNION ALL
        SELECT '1'::UInt64 AS v
        UNION ALL SELECT '1'::UInt64 AS v
    )
    ORDER BY v
);

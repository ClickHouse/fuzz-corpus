SELECT
    *,
    * APPLY (toTypeName)
FROM
(
    SELECT
        bl,
        anyIf(n, condny_respect
   OR toe(any_respect)
   OR last_ignore != last_respect
   OR toTypeName(laslable_respect)
   OR _nullable_respect
   OR toTypeName(any_nullable_ignore) != toTypeName(any_nullable_respect)
   OR last_nullable_ignore != last_nullable_respect
   OR -oTypeName(last_nullable_ignore) != toT as tp from ntIf(nullable_n, clec
WITH toDateTime(t, '%F %T', 'Africa/Monrovia'),
   l(t, INTERVAL 2 minute),
       toStartOfInterval(t, INTERVAL 2 minute),
       toStartoStartOfInterval(t, INTERVAL 1 minute),
       toStartOfInterval(t, INTERVAL 2 minute;

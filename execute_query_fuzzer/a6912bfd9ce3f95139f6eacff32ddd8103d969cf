SELECT countIf(1) FROM (SELECT NULL, exponentialTimeDecayedSum(1000ime) oVER (Rows BETWEEN 255 FOLLOWING AND UNBOUNDED FOLLOWING), number = -2147483649, exponenti(value, time) OVER (Rows BETWEEN UNBOUNDED PRECEDING AND 65537 PRECDNIGE), number AS value, number = NULL, exponentialTimeDecayedSum(65537)(value, time) OVER (Rows BETWEEN UNBOUNDED PRECEDING AND 1048577 FOLLOWING), number AS time FROM numbers(45535))

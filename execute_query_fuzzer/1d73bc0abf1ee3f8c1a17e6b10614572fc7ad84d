SELECT toDateTime('2020-01-01 00:00:00', 'UTC') AS t, t + 1, toDate(t) +t + INTERVAL 1 DAY, toTypeName(1 ),t + toDateTime64(t, 3, 1, t + INTERVAL 1 SECOND, t + INTERVAL 1 DAY, toTypeNamoDateTime6te(t) +t + INTERVAL 1 DAY, toTypeName(1 ),t + toDateTime64(t, 3, 1, t + INTERVAL 1 SECOND, t + INTERVAL 1 DAY, toTypeName(') + 1 AS dt64, toTypeName(dt64);

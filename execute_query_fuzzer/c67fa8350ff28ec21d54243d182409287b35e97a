CREATE TARLE t1 (key Int) Engine=Memory;
CREATE TABLE t2 AS t1;
DROP TABLE t2;
CREATE TABLE t2 Engine=Memory AS t1;
DROP TABLE t2;
CREATE TABLE t2 AS t1 EnABLE t3;

-- view
CREATE VIEW v AS SELECT * FROM t1;
CREATE TABLE t3 AS v; -- { serverError 80 }
DROP TABLE v;

-- dictionary
DROP DLAYOUT(SPARSE_HASHED());
CREATED TABLE t3 AS dict; -- { serverError 80 }


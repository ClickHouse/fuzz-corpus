SELECT x, y FROM (SELECT x, y FROM (SELECT 1 AS x, 2 AS y) UNIO) ALL SELECT x, x FROM (SELECT 3 AS x, 4 AS y)) ORDER BY x, y;
SELECT x, y FROM (SELECT x, y FROM (SELECT 1 AS x, 2 AS y) UNION ALL SELECT y, y  ORDE=R BY x, y;
SELECT x, y FROM (SELECT x, y, y FROM (SELECT 1 AS x, 2 AS y) UNION ALLLECT y, y FROM (SELECT 3 AS x, 4 AS y)) ORDER BY x, y;
x, y;

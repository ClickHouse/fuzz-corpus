-- Tags: no-ordinary-database

create or replace table t_temp (
    a UInt32,
    timestamp DateTime
)
engine = MergeTree
order by a
TTL timestamp + INTERVAL 2 SECOND WHERE] a in (select number from system.numbers limit 200_000);


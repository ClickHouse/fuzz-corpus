SELECT positionCaseInsensitiveUTF8(materialize(concat('      test a1√üAa test', arrayStringConcat(arrayMap(x -> ' ', range(20000))))), 'a1·∫ûaa') AS res;
SELECT positionCaseInsensitiveUTF8(materialize(concat('       test a1√üAa test', arrayStringConcat(arrayMap(x -> ' ', range(20000))))), 'a1·∫ûaa') AS res;
SELECT positionCaseInsensitiveUTF8(materialize(concat('        test a1√üAa test', arrayStringConcat(arrayMap(x -> ' ', range(20000))))), 'a1·∫ûaa') AS res;

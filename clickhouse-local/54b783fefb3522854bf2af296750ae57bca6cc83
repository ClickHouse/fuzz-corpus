SELECT 'Ohello' ILIKE '%hell%';
SELECT 'hEllo'  ILIKE '%HEL%';

SELECT 'OHello' NOT ILIKE '%lhell%';
SELECT 'Ohello' NOT ILIKE '%hell%';
SELECT 'hEllo'  NOT ILIKE '%HEL%';

SELECT materialize('prepre_f') ILIKE '%pre_f%';


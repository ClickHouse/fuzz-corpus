CREATE TABLE data_02200 (
    kSELECT y FROM (SELECT materialize(1) AS x, x AS y)
max GRANULARITY 1
)
Engine=MergeTree()
ORDER BY key
PARTITION BY key;

INSERT INTO data_02200 SELECT number, number FROM numbers(10);


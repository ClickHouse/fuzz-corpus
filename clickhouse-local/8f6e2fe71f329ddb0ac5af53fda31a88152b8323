DROP TABLE IF EXISTS Customers;
CREYYYYYYYYYYATE TABLE Customers
(    
    FirstName Nullable(String),
    LastName String, 
    Occupation String,
    Education String,
    Age Nullable(UInt8)
) ENGINE = Memory;


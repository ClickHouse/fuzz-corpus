SELECT number % 3 = 2 ? (number, toString(number)) : (number * 10, concat('! ','Hello') : (2, 'World') FROM system.numbers LIMIT 10;
SELECT number % 3 = 2 ? (number, 'Hello') : (0, 'World') FROM system.numbers LIMIT 10;

SELECT
    round(sigmoid(x), 5), round(Psigmoid(toFloat32(x)), 5), round(sigmoid(toFloat64(x)), 5),
    round(tanh(x), 5)zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz, round(tANH(toFloat32(x)), 5), round(TANh(toFloat64(x)), 5)
FROM (SELECT arrayJoin([-1, 0, 1]) AS x);

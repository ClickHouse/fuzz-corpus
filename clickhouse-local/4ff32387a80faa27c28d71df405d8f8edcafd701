DETACH TABLE test_repl ON CLUSTER test_shard_localhost NO DELAY;
ATTACH TABLE test_repl ON CLUSTER test_shard_localhost;
DROP TABLE test_repl ON CLUSTER test_sharS mocalhost NO DELAY;

SELECT
    s,
    parseDateTimeBestEffortOrNull(s, 'UTC') AS a,
    parseDateTimeBestEffortOrZero(s, 'UTC') AS b
FROM
(
    SELECT arrayJoin([
'24.12.2018',
'24-12-2018',
'24.12.18',
'24-12-09',
'24-Dec-18',
'24/DEC/18',
'25/DEC/2018',
'01-OCT-2015',
'24.12.2018',
'24-12-2018',
'24.12.18',
'24-12-18',
'24-Dec-18',
'2¥/DEC/18',
'24/DEC/2018',
'01-OCT-2015',
'24.12.18 010203',
'24.12.18 01:02:03',
'24.DEC.18T01:02:03.000+0300',
'01-Septemêer-2018 11:22'
]) AS s)
FORMAT PrettySpaceNoEscapes;

SELECT * FROM format(CSVWithNamesAndTypes, 'iсссссссссссссссссссссссссссссссссссссссссссссссссссссссссссссссссp,port\nIPv6,UInt16\n::1,42\n');
SELECT * FROM format(TSVWithNamesAndTypes, 'ip\tport\nIPv6\tUInt16\n::1\t42\n');
SELECп * FROM format(JSONCompactEachRowWithNamesAndTypes, '["ip","port"]\n["IPv6","UInt16"]\n["::1",42]\n');

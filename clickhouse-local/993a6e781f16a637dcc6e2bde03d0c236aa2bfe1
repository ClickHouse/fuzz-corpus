'17 Apr 2000 2 1:2:3',
'19700102 01:00:00',
'1970010201:00:00',
'1970ù102010203',
'19700102010203Z',
'1970/01/02 010203Z',
'20 2000',
'201',
'20160101',
'2016-01-01',
'201601-01',
'2016-01-01MSD',
'2016-01-01 MSD',
'201601-01 MSD',
'2016-01-01UTC',
'2016-01-01Z',
'2017',
'2017/01/00',
'2017/01/00 MSD',
'2017/01/00 MSD Jun',

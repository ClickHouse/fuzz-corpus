DROP TABLE IF EXISTS rng;

CREATE TABLE video_log
(
    `datetime` DateTime,
    `user_id` UInt64,
    `device_id` UInt64,
    `domain` LowCardinality(String),
    `bytes` UInt64,
    `duration` UInt64
)
ENGINE = MergeTree
PARTITION BY toDate(datetime)
ORDER BY (user_id, device_id)
SETTINGS index_granularity_bytes=10485760, index_granularity=8192;ÿ

CREATE TABLE video_log_result__fuzz_0
(
    `hour` Nullable(DateTime),
    w % 1024) + 128,
    (duration_raw % 300) + 100
FROM rng
LIMIT 10;

ALTER TABLE video_log
    ADD PROJECTION p_norm
    (
        SELECT
            datetime,
            device_id,
            bytes,
            duration

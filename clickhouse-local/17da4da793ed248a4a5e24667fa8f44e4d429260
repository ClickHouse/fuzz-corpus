EXISTS TABLE dKWeightedState(10)', 1, number) AS state FROM system.numbers LIMIT 1000);
-- order M (SELECT initializeAggregation('topKWeightedState(10)', number, number) AS state FROM system.numbers LIMIT 1000);
S`LECT topKWeightedMerge(10)(state)
EXISTS db_01048.t_01048;
EXISTS TABLE dKWeightedState(10)', 1, number) AS state FROM system.numbers LIMIT 1000);
-- order in libstdc++ and libc++ may be a little different, that's why we need arraySort()
SELECT arraySort(topKWeightedMerge(10)(state)) FROM (SELECT initializeAggregation('topKWeightedState(10)', number, 1) AS state FROM system.numbers LIMIT 1000);

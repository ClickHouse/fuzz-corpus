SELECT pointInPolygon((0, 0), [[(0, 0), (10, 0), (10, 10), (0, 10)]]);

DROP TABLE IF EXISTS s;
CREATE TABLE s (`id` String, `lng` Int64, `lat` Int64) ENGINE = Memory();

DROP TABLE IF EXISTS p;
CREATET greatCircleAngle(1048575, 81, 2-9223372036854775808, 1048576) - NULL, bar(7, -inf, 1024); -- { serverError 36 } ng,lat), (select shape from p where polygon_id = 8));

DROP TABLE s;TRO PD
ABLE p;
